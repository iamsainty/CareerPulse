{
    "projects": {
      "name": "career-pulse"
    },
    "builds": [
      {
        "src": "server/server.js",
        "use": "@vercel/node"
      },
      {
        "src": "client/next.config.js",
        "use": "@vercel/next"
      }
    ],
    "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/server.js"
    },
      {
        "src": "/(.*)",
        "dest": "/client/$1"
      }
    ]
  }